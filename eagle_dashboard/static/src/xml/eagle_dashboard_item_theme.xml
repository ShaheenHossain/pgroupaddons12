<?xml version="1.0" encoding="UTF-8"?>

<templates>
    <t t-name="eagle_dashboard_theme_view">
        <div class="eagle_dashboard_theme_view_render">
            <div class="eagle_dashboard_theme_input_container">
                <input class="eagle_dashboard_theme_input eagle_color_white" type="checkbox" name="White Theme"
                       value="white"/>
                <span class="eagle_checkmark"/>
            </div>
            <div class="eagle_dashboard_theme_input_container">
                <input class="eagle_dashboard_theme_input eagle_color_blue" type="checkbox" name="Blue Theme" value="blue"/>
                <span class="eagle_checkmark"/>
            </div>
            <div class="eagle_dashboard_theme_input_container">
                <input class="eagle_dashboard_theme_input eagle_color_red" type="checkbox" name="Red Theme" value="red"/>
                <span class="eagle_checkmark"/>
            </div>
            <div class="eagle_dashboard_theme_input_container">
                <input class="eagle_dashboard_theme_input eagle_color_yellow" type="checkbox" name="Yellow Theme"
                       value="yellow"/>
                <span class="eagle_checkmark"/>
            </div>
            <div class="eagle_dashboard_theme_input_container">
                <input class="eagle_dashboard_theme_input eagle_color_green" type="checkbox" name="Green Theme"
                       value="green"/>
                <span class="eagle_checkmark"/>
            </div>
        </div>
    </t>
</templates>