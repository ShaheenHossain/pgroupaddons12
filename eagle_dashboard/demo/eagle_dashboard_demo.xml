<?xml version="1.0" encoding="utf-8"?>
<eagle>
    <data>

        <!-- Three Default Demo Dashboard with Templates : Template1, Template2, Template3-->

        <record id="demo_template1_dashboard" model="eagle_dashboard.board">
            <field name="name">Template1 Dashboard</field>
            <field name="eagle_dashboard_menu_name">Template1</field>
            <field name="eagle_dashboard_top_menu_id" eval="ref('eagle_dashboard.board_menu_root')"/>
            <field name="eagle_dashboard_default_template" eval="ref('eagle_dashboard.eagle_template_1')"/>
            <field name="eagle_dashboard_active">1</field>
            <field name="eagle_dashboard_group_access" eval="False"/>
        </record>

        <record id="demo_template2_dashboard" model="eagle_dashboard.board">
            <field name="name">Template2 Dashboard</field>
            <field name="eagle_dashboard_menu_name">Template2</field>
            <field name="eagle_dashboard_top_menu_id" eval="ref('eagle_dashboard.board_menu_root')"/>
            <field name="eagle_dashboard_default_template" eval="ref('eagle_dashboard.eagle_template_2')"/>
            <field name="eagle_dashboard_active">1</field>
            <field name="eagle_dashboard_group_access" eval="False"/>
        </record>

        <record id="demo_template3_dashboard" model="eagle_dashboard.board">
            <field name="name">Template3 Dashboard</field>
            <field name="eagle_dashboard_menu_name">Template3</field>
            <field name="eagle_dashboard_top_menu_id" eval="ref('eagle_dashboard.board_menu_root')"/>
            <field name="eagle_dashboard_default_template" eval="ref('eagle_dashboard.eagle_template_3')"/>
            <field name="eagle_dashboard_active">1</field>
            <field name="eagle_dashboard_group_access" eval="False"/>
        </record>
    </data>
</eagle>
